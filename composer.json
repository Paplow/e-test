{
    "name": "e-Test",
    "description": "A micro e-test package for your laravel application.",
    "type": ["library", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Onah Jnr",
            "email": "paplow01@gmail.com"
        }
    ],
    "repository": "https://github.com/Paplow/E-test",
    "require": {
        "php": ">=7.0",
        "illuminate/*": "~5.0",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-0": {
            "Paplow\\eTest": "src/"
        },
        "files": [
            "src/eTest/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paplow\\eTest\\eTestServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
